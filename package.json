{
  "name": "graphql-sqs-subscriptions",
  "version": "0.0.5",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "npm run clean && npx tsc",
    "clean": "rm -rf './dist'",
    "dev": "npx nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "format": "npx prettier --write './src/**/*.{ts,tsx,js,jsx}'",
    "format:check": "npx prettier --check './src/**/*.{ts,tsx,js,jsx}'",
    "lint": "npx eslint './src/**/*.{ts,tsx,js,jsx}'",
    "prepare": "npm run build",
    "test": "npx jest"
  },
  "files": [
    "/dist"
  ],
  "keywords": [
    "amazon web services",
    "apollo",
    "aws",
    "graphql",
    "pubsub",
    "simple queue service",
    "sqs",
    "subscriptions"
  ],
  "author": "John Flockton",
  "license": "MIT",
  "repository": "https://github.com/thegreatercurve/graphql-sqs-subscriptions",
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.2",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "jest": "^24.9.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "@types/uuid": "^3.4.6",
    "aws-sdk": "^2.814.0",
    "graphql-subscriptions": "^1.1.0",
    "uuid": "^3.3.3"
  }
}
